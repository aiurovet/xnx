#!/bin/bash

################################################################################

DIR="${1}"
INP="${2}"

################################################################################

if [[ "${DIR}" != "" ]] && [[ "${INP}" != "" ]]; then
  mkdir -p "${DIR}" &&\
  unzip -o -d "${DIR}" "${INP}" &&\
  exit 0
fi

################################################################################

echo "Failed to unzip \"${INP}\" to \"${DIR}\""
exit 1

################################################################################
