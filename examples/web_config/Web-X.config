<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <configSections>
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
  </configSections>
  <appSettings>
 
    <add key="Logging:Path" value="C:\\logs\\{EnvSite}\\AppName\\"/>
    <add key="Logging:FileName" value="log-{Date}"/>
 
    <add key="connectionName" value="DBContext_{Env}_{Site}" />
    <add key="serviceUrlName" value="{ServiceUrl-{Env-Site}}" />
    <add key="firstApiUrlName" value="{FirstApiUrl-{Env-Site}}" />
    <add key="secondApiUrlName" value="{SecondApiUrl-{Env-Site}}" />
    <add key="thirdApiUrlName" value="{ThirdApiUrl-{Env-Site}}" />
    <add key="fourthApiUrlName" value="{FourthApiUrl-{Env-Site}}" />
    <add key="fifthApiUrlName" value="{FifthApiUrl-{Env-Site}}" />
    <add key="sixthApiUrlName" value="{SixthApiUrl-{Env-Site}}" />
    <!--!ends with /-->
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="PreserveLoginUrl" value="true" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <!-- AWS S3 -->
    <add key="S3Bucket" value="{envsite}-AppName-business" />
    <add key="S3UploadDir" value="01" />
  </appSettings>
    <connectionStrings>
    <add name="DBContext_{Env}_{Site}" connectionString="substituted by AzDO variables" providerName="System.Data.SqlClient" />
    </connectionStrings>
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.
 
    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.6.1" />
      </system.Web>
  -->
  <system.web>
    <customErrors mode="Off"/>
      <globalization culture="{culture}" uiCulture="{culture}" />
    <compilation targetFramework="4.6.1" />
    <httpCookies requireSSL="true" />
    <authentication mode="Forms">
      <!-- From client's browser perspective, to ensure authentication cookies of different apps don't clash with each other, the value of attribute 'name' should be unique per environment/business/application -->
      <forms loginUrl="~/Account/Login" timeout="2880" cookieless="UseCookies" requireSSL="false" name="CookieName"/>
    </authentication>
    <machineKey validation="SHA1" validationKey="myValidationKey" decryption="AES" decryptionKey="myDecryptionKey" />
    <pages controlRenderingCompatibilityVersion="4.0">
      <namespaces>
        <add namespace="System.Web.Helpers" />
        <add namespace="System.Web.Mvc" />
        <add namespace="System.Web.Mvc.Ajax" />
        <add namespace="System.Web.Mvc.Html" />
        <add namespace="System.Web.Optimization" />
        <add namespace="System.Web.Routing" />
        <add namespace="System.Web.WebPages" />
        <add namespace="Kendo.Mvc.UI" />
      </namespaces>
    </pages>
  </system.web>
  <system.webServer xdt:Transform="Replace">
    <validation validateIntegratedModeConfiguration="false" />
    <modules runAllManagedModulesForAllRequests="true" />
    <!-- rewrite node should be uncommented only if USE_VERSION_AS_QUERY_PARAM is defined -->
    <!--<rewrite>
      <rules>
        <rule name="fingerprint" stopProcessing="true">
          <match url="([\S]+)(/[\S]+_v-[0-9_\-]+/)([\S]+)" ignoreCase="false" />
          <action type="Rewrite"  url="{R:1}/{R:3}" />
        </rule>
      </rules>
    </rewrite>-->
    <staticContent>
      <!-- keep in cache for 1 year (this is more than enough) -->
      <clientCache cacheControlMode="UseMaxAge" cacheControlMaxAge="365.00:00:00" />
    </staticContent>
  <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers>
    <httpProtocol>
      <customHeaders>
        <remove name="X-Frame-Options" />
        <add name="X-Frame-Options" value="DENY" />
      </customHeaders>
    </httpProtocol>
  </system.webServer>
