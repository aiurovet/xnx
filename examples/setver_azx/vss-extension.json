{
  "manifestVersion": 1,
  "id": "xnx-azx",
  "publisher": "aiurovet",
  "version": "0.1.44",
  "name": "xnx",
  "description": "Azure DevOps extension for the xnx utility",
  "public": false,
  "categories": [ "Azure Repos" ],
  "targets": [ { "id": "Microsoft.VisualStudio.Services" } ],
  "icons": { "default": "images/extension-icon.png" },
  "contributions": [
    {
      "id": "xnx-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [ "ms.vss-distributed-task.tasks" ],
      "properties": { "name": "bin" }
    }
  ],
  "files": [
    { "path": "bin" },
    { "path": "task.json", "packagePath": "bin/task.json" },
    { "path": "images/extension-icon.png", "packagePath": "bin/extension-icon.png" },
    { "path": "bin/Linux/xnx", "packagePath": "bin/Linux/xnx" },
    { "path": "bin/macOS/xnx", "packagePath": "bin/macOS/xnx" },
    { "path": "bin/Windows/xnx.exe", "packagePath": "bin/Windows/xnx" }
  ]
}