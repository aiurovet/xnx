{
  "{{-rename-keywords-}}": {
    "{{-cmd-}}": "{c}",
    "{{-inp-}}": "{i}",
    "{{-out-}}": "{o}",
    "{{-run-}}": "{r}",
    "{{-sub-}}": "{s}"
  },

  "{{-import-}}": "../cmd.json",

  "{OsType}": "?",

  "{{-if-}}": { "!=": [ "${HOME}", "" ], "{{-then-}}": {
    "{OsType}": "Linux",
    "{{-if-}}": { "-e": [ "/Applications/*.app" ], "{{-then-}}": {
      "{OsType}": "macOS",
    } }
  }, "{{-else-}}": { "{{-if-}}": { "!=": [ "${UserProfile}", "" ], "{{-then-}}": {
    "{OsType}": "Windows"
  } } } },

  "{r}": '{s} --echo "OS type: {OsType}"'
}
