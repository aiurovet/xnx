{
  "{{-aliases-}}": {
    "{{-cmd-}}": "{c}",
    "{{-inp-}}": "{i}",
    "{{-out-}}": "{o}",
    "{{-run-}}": "{r}"
  },

  "{OsType}": "?",

  "{{-if-}}": { "!=": [ "${HOME}", "" ], "{{-then-}}": {
    "{OsType}": "Linux",
    "{{-if-}}": { "-e": [ "/Applications/*.app" ], "{{-then-}}": {
      "{OsType}": "macOS"
    } }
  }, "{{-else-}}": { "{{-if-}}": { "!=": [ "${UserProfile}", "" ], "{{-then-}}": {
    "{OsType}": "Windows"
  } } } },

  "{r}": "echo OS type: {OsType}"
}
