{
  "rename": {
    "{{-can-expand-content-}}": "{CEC}",
    "{{-cmd-}}": "{c}",
    "{{-cur-dir-}}": "{CD}",
    "{{-expand-content-}}": "{expand-content}",
    "{{-inp-}}": "{i}",
    "{{-inp-dir-}}": "{id}",
    "{{-inp-ext-}}": "{ix}",
    "{{-inp-name-}}": "{in}",
    "{{-out-}}": "{o}"
  },
  "action": [
    { "{CEC}": true },
    { "{c}": "{expand-content} \"{i}\" \"{o}\"" },

    { "{i}": "{CD}/template/*settings.json" },

    { "{env}": [
        { "{envTC}": "Dev",  "{envLC}": "dev"  },
        { "{envTC}": "Test", "{envLC}": "test" },
        { "{envTC}": "Prod", "{envLC}": "prod" }
    ] },

    { "{dir}": "${HOME}${USERPROFILE}/{envLC}", "{o}": "{id}/../out/{in}-{envLC}{ix}" },
  ]
}
