{
  "{{-rename-keywords-}}": {
    "{{-cmd-}}": "{c}",
    "{{-cur-dir-}}": "{cur-dir}",
    "{{-can-expand-content-}}": "{can-expand-content}",
    "{{-inp-}}": "{i}",
    "{{-out-}}": "{o}"
  },

  "{{-import-}}": "../cmd.json",

  // Do not enclose the output path in quotes, as this will not work, hence avoid spaces (existing bug in Chromium)
  "{c}": "{chrome} --headless --disable-gpu --default-background-color=0 --window-size={d},{d} --screenshot={o} \"file://{i}\"",

  "{can-expand-content}": true,
  "{img-src-dir}": "{cur-dir}/svg",

  "{i}": "{cur-dir}/resizer.svg",
  "{t}": [ "edit", "find", "save" ],
  "{d}": [ 20, 40, 80, 160 ],
  "{o}": "{cur-dir}/png/icon-{t}-{d}x{d}.png"
}
