{
  "{{-import-}}": "../shell.xnx",

  "{{-cmd-}}": "{svg2png}",

  "{{-can-expand-content-}}": true,
  "{{-detect-paths-}}": "\\{[^\\{\\}]+\\-(dir|path|pthp)\\}",

  "{img-src-dir}": "{{-cur-dir-}}/svg",

  "{typ}": [ "edit", "find", "save" ],
  "{dim}": [ 20, 40, 80, 160 ],
  "{{-func-}}": {
    "{scale}": [ "=Div", "{dim}", "100" ],
  },
  "{{-inp-}}": "{{-cur-dir-}}/svg/{typ}.svg",
  "{{-out-}}": "{{-cur-dir-}}/out/icon-{typ}-{dim}x{dim}.png",
}
