#!/bin/bash

################################################################################

PRJ_NAME="doul"
PRJ_EXE="${SCR_DIR}/../bin/linux/${PRJ_NAME}"
ICO_DIR=$( dirname "${0}" )

################################################################################

if [ ! -f "${PRJ_EXE}" ]; then
  "${SCR_DIR}/build" || exit 1
fi

"${BIN}" -d "${ICO_DIR}" -c mkicons-00,mkicons,mkicons-99 ${@}

exit ${?}

################################################################################
